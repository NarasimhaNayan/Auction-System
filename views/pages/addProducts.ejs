<body>
    <!-- ----------------------- product form -------------------- -->
    <section class="user">
        <header class="user__header">
            <h1>Welcome!,
                <%= USER %>
            </h1>
            <%= typeof MSG!= 'undefined' ? MSG : '' %>
                <img src="data:image/png;base64,"
                />
                <h1 class="user__tittle">Add Product</h1>
        </header>
        <form method="POST" action="/products/add-product" enctype="multipart/form-data" class="form">
            <div class="form__group">
                <select class="form__input selectpicker" data-style="btn-info" name="selectpicker">
                <option name="" value="">Category</option>
                <option name="a" value="Mobile">Mobile</option>
                <option name="b" value="Car">Car</option>
                <option name="c" value="Bikes">Bikes</option>
                <option name="d" value="Electronics">Electronics</option>
              </select>
            </div>
            <div class="form__group">
                <input type="text" class="form__input" placeholder="Name" name="Name" required="required" />
